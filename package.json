{
  "name": "vue-dynamic-property-provider",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-dynamic-property-provider.umd.js",
  "module": "./dist/vue-dynamic-property-provider.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-dynamic-property-provider.es.js",
      "require": "./dist/vue-dynamic-property-provider.umd.js"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.vue .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|vue)\""
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.38",
    "prettier": "^2.4.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
